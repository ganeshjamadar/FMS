{
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:5000"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        }
      },
      "profile-route": {
        "ClusterId": "identity",
        "Match": {
          "Path": "/api/profile/{**catch-all}"
        }
      },
      "invitations-route": {
        "ClusterId": "fundadmin",
        "Match": {
          "Path": "/api/invitations/{**catch-all}"
        }
      },
      "contributions-route": {
        "ClusterId": "contributions",
        "Match": {
          "Path": "/api/funds/{fundId}/contributions/{**catch-all}"
        }
      },
      "loans-route": {
        "ClusterId": "loans",
        "Match": {
          "Path": "/api/funds/{fundId}/loans/{**catch-all}"
        }
      },
      "dissolution-route": {
        "ClusterId": "dissolution",
        "Match": {
          "Path": "/api/funds/{fundId}/dissolution/{**catch-all}"
        }
      },
      "audit-route": {
        "ClusterId": "audit",
        "Match": {
          "Path": "/api/funds/{fundId}/audit/{**catch-all}"
        }
      },
      "notifications-route": {
        "ClusterId": "notifications",
        "Match": {
          "Path": "/api/notifications/{**catch-all}"
        }
      },
      "fundadmin-route": {
        "ClusterId": "fundadmin",
        "Match": {
          "Path": "/api/funds/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "identity": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5001"
          }
        }
      },
      "fundadmin": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5002"
          }
        }
      },
      "contributions": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5003"
          }
        }
      },
      "loans": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5004"
          }
        }
      },
      "dissolution": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5005"
          }
        }
      },
      "notifications": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5006"
          }
        }
      },
      "audit": {
        "Destinations": {
          "default": {
            "Address": "http://localhost:5007"
          }
        }
      }
    }
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*"
}
